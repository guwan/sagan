{
    "name": "spring.io",
    "description": "bower components for spring.io UI.",
    "dependencies": {
        "when": "~2",
        "meld": "~1",
        "most": "~0.2",
        "curl": "~0.8",
        "poly": "~0",
        "jquery": "~2.2.1",
        "bootstrap": "~2.3",
        "bootstrap-datetimepicker": "0.0.10",
        "font-awesome": "3.2.1",
        "gmaps": "0.4.14",
        "zeroclipboard": "1.1.7",
        "google-code-prettify": "~1.0",
        "jquery-timeago": "1.4.1",
	"amcharts":"3.15.2",
        "angularjs": "1.5.0",
        "backstretch": "2.0.4",
        "bootbox": "4.4.0",
        "bootstrap": "~3.3.6",
        "bootstrap-colorpicker": "~",
        "bootstrap-datepaginator": "~1.2.0",
        "bootstrap-datepicker": "1.6.0",
        "bootstrap-daterangepicker": "2.1.18",
        "bootstrap-datetimepicker": "0.0.11",
        "bootstrap-editable": "~",
        "bootstrap-fileinput": "~4.3.1",
        "bootstrap-hover-dropdown": "2.2.1",
        "bootstrap-markdown": "2.10.0",
        "bootstrap-maxlength": "1.7.0",
        "bootstrap-modal": "2.2.6",
        "bootstrap-pwstrength": "1.5.0",
        "bootstrap-select": "1.5.0",
        "bootstrap-sessiontimeout": "1.5.0",
        "bootstrap-summernote": "1.5.0",
        "bootstrap-switch": "1.5.0",
        "bootstrap-timepicker": "1.5.0",
        "bootstrap-toastr": "1.5.0",
        "bootstrap-touchspin": "1.5.0",
        "bootstrap-wizard": "1.5.0",
        "bootstrap-wysihtml5": "1.5.0",
        "carousel-owl-carousel": "1.5.0",
        "angularjs": "1.5.0"
    },
    "overrides": {
        "jquery": {
            "ignore": ["*.json"]
        },
        "curl": {
            "ignore": ["dist/**", "src/curl/**"]
        },
        "meld": {
            "ignore": ["aspect/**", "docs/**", "test/**"]
        },
        "most": {
            "ignore": ["test/**"]
        },
        "bootstrap": {
            "ignore": [
                "js/**", "img/**", "less/**", "docs/build/**",
                "docs/examples/**", "docs/templates/**", "docs/*.html"
            ]
        },
        "bootstrap-datetimepicker": {
            "ignore": ["build/**", "test/**"]
        },
        "font-awesome": {
            "ignore": ["css/**", "less/**", "scss/**", "src/**"]
        },
        "gmaps": {
            "ignore": ["examples/**", "test/**"]
        },
        "google-code-prettify": {
            "ignore": ["styles/**"]
        },
        "zeroclipboard": {
            "ignore": ["docs/**", "src/**", "test/**"]
        },
        "bootstrap*": {
            "ignore": [
                "js/**", "img/**", "less/**", "docs/build/**",
                "docs/examples/**", "docs/templates/**", "docs/*.html"
            ]
        }
    }
}
