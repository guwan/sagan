{
    "name": "spring.io",
    "description": "bower components for spring.io UI.",
    "dependencies": {
        "when": "~2",
        "meld": "~1",
        "most": "~0.2",
        "curl": "~0.8",
        "poly": "~0",
        "jquery": "2.0.3",
        "bootstrap": "3.3.6",
        "bootstrap-datepicker": "1.6.0",
        "font-awesome": "4.5.0",
        "gmaps": "0.4.14",
        "zeroclipboard": "1.1.7",
        "google-code-prettify": "~1.0",
        "jquery-timeago": "1.4.1",
        "fancybox":"2.1.5",
        "owl-carousel":"1.3.2",
        "layerslider":"5.0.4",
        "jquery-zoom":"1.7.15",
        "bootstrap-touchspin":"3.0.1",
        "jquery-slimscroll":"1.3.7",
        "backToTop":"1.0.2",
        "webtorrent":"0.90.3"
    },
    "overrides": {
        "jquery": {
            "ignore": ["*.json"]
        },
        "curl": {
            "ignore": ["dist/**", "src/curl/**"]
        },
        "meld": {
            "ignore": ["aspect/**", "docs/**", "test/**"]
        },
        "most": {
            "ignore": ["test/**"]
        },
        "bootstrap": {
            "ignore": [
                "js/**", "img/**", "less/**", "docs/build/**",
                "docs/examples/**", "docs/templates/**", "docs/*.html"
            ]
        },
        "bootstrap-datetimepicker": {
            "ignore": ["build/**", "test/**"]
        },
        "font-awesome": {
            "ignore": ["css/**", "less/**", "scss/**", "src/**"]
        },
        "gmaps": {
            "ignore": ["examples/**", "test/**"]
        },
        "google-code-prettify": {
            "ignore": ["styles/**"]
        },
        "zeroclipboard": {
            "ignore": ["docs/**", "src/**", "test/**"]
        }
    }
}